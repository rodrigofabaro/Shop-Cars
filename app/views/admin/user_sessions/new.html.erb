<% form_for([:admin, @user_session], :url => admin_user_sessions_path, :html => {:multipart => true}) do |f| %>

	<p>
		Login<br>
		<%= f.text_field :login %>
	</p>
	<p>
		Senha<br>
		<%= f.password_field :password %><br />
	</p>
	<p>
	   <%= f.submit "Entrar" %> |
	   <%= link_to "esqueci a senha", new_admin_password_reset_path %>
	</p>


<% end %>

