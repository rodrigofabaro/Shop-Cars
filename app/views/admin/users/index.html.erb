<h1>Lista dos Usuarios</h1>

<%= link_to 'Adicionar um Usuario', new_admin_user_path(@car) %>

<table>
  <tr>
    <th>Login</th>
    <th>Email</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%=h user.login %></td>
    <td><%=h user.email %></td>
    <td><%= link_to 'Show', admin_user_path(user) %> |
        <%= link_to 'Edit', edit_admin_user_path(user) %> |
        <%= link_to 'Destroy', [:admin, user], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

